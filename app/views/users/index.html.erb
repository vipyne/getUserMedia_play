<div class="container">

  <ul>
    <li>
      <button class="github">github streak</button>
      <% @bam.each do |hash|%>
      <p><%= hash["avatar_url"]%></p>
      <% end %>
    </li>
    <li>
      <form class="your-name">
        <input autofocus type="text" placeholder=" your name">
      </form>
    </li>
    <li>
      <button class="click">see yourself</button>
    </li>
    <li>
      <div class="name">
      </div>
    </li>
    <li>
      <div class="your-video">
      </div>
      <video autoplay></video>
      <div class="my-border">
      </div>
    </li>
  </ul>

</div>

<script id="your-name" type="text/x-handlebars-template">
{{name}}
</script>
